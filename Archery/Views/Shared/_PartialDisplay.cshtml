@using Archery.Tools;

@{
    var message = TempData["MESSAGE"] as Archery.Tools.Message;

}


@if (message != null)
{
    <div class="alert
                     alert-@(message.MessageType==MessageType.SUCCESS?"success":"danger:")">
        <!----pour utiliser le message d'alerte qqsoit le type success, error...-->
        @message.Text
    </div>
}
